{
  "expo": {
    "name": "sun-and-market",
    "slug": "sun-and-market",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/sundays-icon.png",
    "scheme": "mobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pdominguez.sunandmarket",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "icon": {
        "dark": "./assets/images/sundays-icon.png",
        "light": "./assets/images/sundays-icon.png",
        "tinted": "./assets/images/sundays-icon.png"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/sundays-icon.png",
        "backgroundColor": "#ffffff",
        "backgroundImage": "./assets/images/sundays-icon.png"
      },
      "package": "com.paulsundays.sunandmarket",
      "versionCode": 1
    },
     "submit": {
      "production": {
        "ios": {
          "ascAppId": "6747617280"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "e5d123d4-709b-46a9-8cdd-8b539e988ad9"
      }
    },
    "owner": "pdominguez0998expo"
  }
}
